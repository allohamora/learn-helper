{
  "name": "learn-helper",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev --remote",
    "start": "node ./dist/server/entry.mjs",
    "build": "astro build --remote",
    "postbuild": "sh ./scripts/create-index.sh",
    "check": "astro check",
    "preview": "astro preview",
    "push": "astro db push --remote",
    "seed": "astro db execute db/seed.ts --remote",
    "astro": "astro",
    "prepare": "husky",
    "format": "prettier . --check",
    "format:fix": "prettier --write .",
    "lint": "eslint \"**/*.{ts,tsx,astro}\"",
    "lint:fix": "eslint \"**/*.{ts,tsx,astro}\" --fix",
    "csslint": "stylelint \"src/**/*.{css,ts,tsx}\"",
    "csslint:fix": "stylelint \"src/**/*.{css,ts,tsx}\" --fix",
    "test": "vitest",
    "test:integration": "vitest --config vitest-integration.config.ts",
    "test:evals": "vitest --config vitest-evals.config.ts"
  },
  "dependencies": {
    "@ai-sdk/google": "^2.0.51",
    "@astrojs/db": "^0.19.0",
    "@clerk/astro": "^2.16.17",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.18",
    "ai": "^5.0.116",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "rate-limiter-flexible": "^9.1.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-hook-form": "^7.71.1",
    "react-scan": "^0.4.3",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "^1.4.0",
    "type-fest": "^5.4.3",
    "use-debounce": "^10.1.0",
    "usehooks-ts": "^3.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@ai-sdk/openai": "^2.0.88",
    "@astrojs/check": "^0.9.6",
    "@astrojs/node": "^9.5.2",
    "@astrojs/react": "^4.4.2",
    "@commitlint/cli": "^20.4.0",
    "@commitlint/config-conventional": "^20.4.0",
    "@eslint/js": "^9.39.2",
    "@tailwindcss/vite": "^4.1.18",
    "@types/dom-speech-recognition": "^0.0.7",
    "@types/eslint-plugin-jsx-a11y": "^6.10.1",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@types/react": "19.2.10",
    "@types/react-dom": "19.2.3",
    "astro": "^5.17.1",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.5.0",
    "eslint-plugin-beautiful-sort": "^3.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss-styled-syntax": "^0.7.1",
    "prettier": "^3.8.1",
    "prettier-plugin-astro": "^0.14.1",
    "stylelint": "^16.26.1",
    "stylelint-config-clean-order": "^8.0.0",
    "stylelint-config-standard": "^39.0.1",
    "stylelint-config-tailwindcss": "^1.0.0",
    "stylelint-prettier": "^5.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "3.2.4"
  },
  "engines": {
    "node": ">=v22.13.0",
    "npm": ">=10.9.2"
  },
  "packageManager": "npm@10.9.2",
  "lint-staged": {
    "*.{js,cjs,mjs,json,yml,md}": "prettier --write",
    "*.css": "stylelint --fix",
    "*.{ts,tsx,astro}": "eslint --fix"
  }
}
